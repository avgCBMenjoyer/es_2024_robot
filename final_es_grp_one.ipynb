    "skel_widget_update = widgets.Image(width=camera.width, height=(camera.height))\n",
    "grn_widget = widgets.Image(width=camera.width, height=(camera.height))\n",
    "# Constants\n",
    "frame_center_x = 60 # Center of the frame\n",
    "base_speed = 0.1                    # Base speed of the robot it was 0.11 first\n",
    "turn_sensitivity = 0.0012            # Turning sensitivity factor it was 0.0012 first\n",
    "sharp_turn_threshold = 0.3          # Proportion of frame width for sharp turns it was 0.5 first\n",
    "line_lost_timeout = 5             # Time to keep turning if the line is lost\n",
    "full_stop_timeout = 4            #time to stop going straight\n",
    "turn_threshold = 0.2             #threshold to consider a turn\n",
    "\n",
    "# HSV range for black\n",
    "lower_black = np.array([0, 0, 20])\n",
    "upper_black = np.array([250, 250, 100])\n",
    "\n",
    "#HSV range for Green\n",
    "lower_green = np.array([35, 40, 85])  # Lower HSV bound for green\n",
    "upper_green = np.array([85, 255, 255])  # Upper HSV bound for green\n",
    "\n",
    "#HSV Ranges for Red (lower and upper sections)\n",
    "lower_red1 = np.array([0, 100, 100])\n",
    "upper_red1 = np.array([10, 255, 255])\n",
    "lower_red2 = np.array([160, 100, 100])\n",
    "upper_red2 = np.array([179, 255, 255])\n",
    "\n",
    "\n",
    "#Image Widgets\n",
    "hsv_image_widget = widgets.Image(width=camera.width, height=(camera.height))\n",
    "hsv_image_widget_green = widgets.Image(width=camera.width, height=(camera.height))\n",
    "merged_image_widget = widgets.Image(width=camera.width, height=(camera.height))\n",
    "left_region_widget = widgets.Image(width=camera.width, height=(camera.height))\n",
    "right_region_widget = widgets.Image(width=camera.width, height=(camera.height))\n",
    "middle_region_widget = widgets.Image(width=camera.width, height=(camera.height))\n",
    "below_region_widget = widgets.Image(width=camera.width, height=(camera.height))\n",
    "# y_coord_label = widgets.Image(width=camera.width, height=(camera.height))\n",
    "\n",
    "#Text Widgets\n",
    "middle_region_black_centroid = widgets.Text(value=\"Hello <b>World</b>\", disabled=False)\n",
    "lefty_green = widgets.Text(value=\"Hello <b>World</b>\", disabled=False)\n",
    "lefty_black = widgets.Text(value=\"Hello <b>World</b>\", disabled=False)\n",
    "left_grn_centroid = widgets.Text(value=\"bruh\", disabled=False)\n",
    "right_grn_centroid = widgets.Text(value=\"bruh\", disabled=False)\n",
    "# status_status_displayssage = widgets.Text(value=\"bruh\", disabled=False)\n",
    "status_display = widgets.Text(value=\"bruh\", disabled=False)\n",
    "final_message = widgets.Text(value=\"Bye\", disabled=False)\n",
    "Detect_obstacle = widgets.Text(value=\"Free\", disabled=False)\n",
    "\n",
    "SharpTurn = widgets.Text(value=\"Nothing\", disabled=False)\n",
    "\n",
    "#Hardcoded Function For The Green Boxes\n",
    "\n",
    "robot = Robot() #Initialize the instance robot using the imported Robot Class\n",
    "\n",
    "#Will make a 90 Degree Turn to the Left\n",
    "def TurnLeftNinetyDegrees():\n",
    "    #Make the 90 degree turn then return to the original settings\n",
    "    robot.set_motors(-0.05, 0.3)\n",
    "    time.sleep(0.3)\n",
    "    robot.stop()\n",
    "    #robot.set_motors(0.215, 0.2)\n",
    "\n",
    "\n",
    "#robot.stop()\n",
    "\n",
    "#Will make a 90 Degree Turn to the Right\n",
    "def TurnRightNinetyDegrees():\n",
    "    #Make the 90 degree turn then return to the original settings\n",
    "    robot.set_motors(0.3, -0.05)\n",
    "    time.sleep(0.3)\n",
    "    robot.stop()\n",
    "    #robot.set_motors(0.215, 0.2)\n",
    "\n",
    "#Will make a 180 Degree-Turn to the Left, or a U-Turn\n",
    "def UTurnLeft():\n",
    "    #Make the 90 degree turn then return to the original settings\n",
    "    robot.set_motors(-0.2, 0.2)\n",
    "    time.sleep(0.7)\n",
    "    robot.stop()\n",
    "    #robot.set_motors(0.215, 0.2)\n",
    "\n",
    "def find_halt(big_img):\n",
    "    halt = False\n",
    "    img_hsv = cv2.cvtColor(big_img, cv2.COLOR_BGR2HSV)\n",
    "    mask1 = cv2.inRange(img_hsv, lower_red1, upper_red1)\n",
    "    mask2 = cv2.inRange(img_hsv, lower_red2, upper_red2)\n",
    "    mask = mask1 + mask2\n",
    "    contours, h = cv2.findContours(mask,1,cv2.CHAIN_APPROX_NONE)\n",
    "    if len(contours) > 0:\n",
    "        c = max(contours, key=cv2.contourArea)\n",
    "        if (cv2.contourArea(c)) > 5000:\n",
    "            halt = True\n",
    "    return halt\n",
    "\n",
    "def detect_obstacle(hsv_img):\n",
    "    area_threshold = 5000\n",
    "    \n",
    "#     while True:\n",
    "#         my_img = camera.value\n",
    "#         # Convert image to HSV\n",
    "#         hsv_img = cv2.cvtColor(my_img, cv2.COLOR_BGR2HSV)\n",
    "\n",
    "        # Define range for obstacle detection (adjust as needed)\n",
    "#         lower_range = np.array([0, 0, 0])\n",
    "#         upper_range = np.array([180, 255, 30])\n",
    "    lower_range = np.array([0, 0, 0])\n",
    "    upper_range = np.array([180, 255, 50])\n",
    "\n",
    "    # Create mask\n",
    "    obstacle_mask = cv2.inRange(hsv_img, lower_range, upper_range)\n",
    "\n",
    "    # Find contours\n",
    "    contours, _ = cv2.findContours(obstacle_mask, cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)\n",
    "\n",
    "    # Find largest contour\n",
    "    if contours:\n",
    "        largest_contour = max(contours, key=cv2.contourArea)\n",
    "        area = cv2.contourArea(largest_contour)\n",
    "\n",
    "        # Check if contour area is above threshold\n",
    "        if area > area_threshold:\n",
    "            Detect_obstacle.value = \"Blocked!\"\n",
    "            return True\n",
    "        else:\n",
    "            Detect_obstacle.value = \"Free!\"\n",
    "            return False\n",
    "\n",
    "\n",
    "\n",
    "def linecenter(black_mask):\n",
    "    M = cv2.moments(black_mask)\n",
    "    if M[\"m00\"] == 0:\n",
    "        return None\n",
    "    return M[\"m10\"]/M[\"m00\"] #m10/m00 means we are giving attention to the x-coordinate\n",
    "\n",
    "\n",
    "    \n",
    "    #Additional Function (Added by Zayed)\n",
    "def linecenterYCoordinate(black_mask):\n",
    "    M = cv2.moments(black_mask)\n",
    "    if M[\"m00\"] == 0:\n",
    "        return None\n",
    "    #return M[\"m10\"]/M[\"m00\"] #m10/m00 returns the x-coordinate of the centroid\n",
    "    return M[\"m01\"]/M[\"m00\"] #m10/m00 returns the y-coordinate of the centroid\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def max_cnt(le_mask):\n",
    "    contours, h = cv2.findContours(le_mask, 1, cv2.CHAIN_APPROX_NONE)\n",
    "    cx = None\n",
    "    cy = None\n",
    "    daddy = []\n",
    "    if len(contours) > 0 :\n",
    "        c = max(contours, key=cv2.contourArea)\n",
    "        Cock = cv2.moments(c)\n",
    "        if Cock[\"m00\"] !=0:\n",
    "            cx = int(Cock['m10']/Cock['m00'])\n",
    "            cy = int(Cock['m01']/Cock['m00'])\n",
    "    return cx,cy\n",
    "\n",
    "def line_y(black_mask):\n",
    "    M = cv2.moments(black_mask)\n",
    "    if M[\"m00\"] == 0:\n",
    "        return None\n",
    "    return int(M[\"m01\"]/M[\"m00\"]) #m01/m00 means we are giving attention to the y-coordinate\n",
    "\n",
    "# Function to find the line center\n",
    "def find_line_center(binary_mask):\n",
    "    M = cv2.moments(binary_mask)\n",
    "    if M[\"m00\"] == 0:  # No line detected\n",
    "        return None\n",
    "    return int(M[\"m10\"] / M[\"m00\"])  # Calculate x-coordinate of line center\n",
    "# Function to follow the line\n",
    "\n",
    "def find_skeletal(blk):\n",
    "    blk_gray = cv2.cvtColor(blk, cv2.COLOR_BGR2GRAY)\n",
    "    # Otsu's thresholding after Gaussian filtering\n",
    "    blur = cv2.GaussianBlur(blk_gray,(5,5),0)\n",
    "    ret_blk,th_blk = cv2.threshold(blur,0,255,cv2.THRESH_BINARY+cv2.THRESH_OTSU)\n",
    "    img = th_blk.copy()\n",
    "    size = np.size(img)\n",
    "    skel = np.zeros(img.shape,np.uint8)\n",
    "    element = cv2.getStructuringElement(cv2.MORPH_CROSS,(3,3))\n",
    "    done = False\n",
    "    #erode the image bit by bit until a width of 1 pixel is reached\n",
    "    while( not done):\n",
    "        eroded = cv2.erode(img,element)\n",
    "        temp = cv2.dilate(eroded,element)\n",
    "        temp = cv2.subtract(img,temp)\n",
    "        skel = cv2.bitwise_or(skel,temp)\n",
    "        img = eroded.copy()\n",
    "\n",
    "        zeros = size - cv2.countNonZero(img)\n",
    "        if zeros==size:\n",
    "            done = True\n",
    "    return skel\n",
    "\n",
    "\n",
    "\n",
    "anomaly_flag=0   #used to check whether any non-normal conditions are detected (turns, obstacles)\n"
   ]
