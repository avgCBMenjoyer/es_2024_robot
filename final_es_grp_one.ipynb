    "def combined_follow():\n",
    "    global anomaly_flag\n",
    "    condition = None\n",
    "    last_line_center = frame_center_x  # Keep track of the last detected line center\n",
    "    last_detected_time = time.time()  # Time when the line was last detected\n",
    "    \n",
    "    CloseSharpTurnsFlag = 0\n",
    "    TurnStatus = 0 #0 Straight, 1 left, 2 right\n",
    "    \n",
    "    while anomaly_flag == 0:\n",
    "        green_turned = 0\n",
    "        my_img = camera.value\n",
    "        if find_halt(my_img):\n",
    "            robot.stop()\n",
    "            anomaly_flag = 2\n",
    "            break\n",
    "        my_img_hsv = cv2.cvtColor(my_img.copy(), cv2.COLOR_BGR2HSV)\n",
    "        if detect_obstacle(my_img_hsv):\n",
    "            robot.stop()\n",
    "            continue\n",
    "        #green_img = enhance_green(my_img.copy())\n",
    "        lefty = my_img.copy() #for copying as python calls do not edit copies normally\n",
    "        righty = my_img.copy()\n",
    "        cropped_frame = my_img[(camera.height //3):(2 * camera.height//3), (camera.width//4-4):(3 * camera.width//4 + 4)] #main cropped frame\n",
    "        cropped_left = lefty[(4 * camera.height //8):(6 * camera.height//8), (camera.width//4 - 4):(camera.width//2 - 3)] #for left green box\n",
    "        cropped_right = righty[(4 * camera.height //8):(6 * camera.height//8), (camera.width//2 + 3):(3 * camera.width//4 + 4)] #for right green box\n",
    "        # Convert to HSV and create the black and green masks (including left and right regions)\n",
    "        hsv_frame_blk = cv2.cvtColor(cropped_frame, cv2.COLOR_BGR2HSV)\n",
    "        hsv_frame_grn = hsv_frame_blk.copy()\n",
    "        hsv_frame_grn_l = cv2.cvtColor(cropped_left, cv2.COLOR_BGR2HSV)\n",
    "        hsv_frame_grn_r = cv2.cvtColor(cropped_right, cv2.COLOR_BGR2HSV)\n",
    "        mask_grn_l = cv2.inRange(hsv_frame_grn_l, lower_green, upper_green)\n",
    "        mask_grn_r = cv2.inRange(hsv_frame_grn_r, lower_green, upper_green)\n",
    "        mask_grn = cv2.inRange(hsv_frame_grn, lower_green, upper_green)\n",
    "        mask_blk = cv2.inRange(hsv_frame_blk, lower_black, upper_black)\n",
    "        black_filter = cv2.bitwise_and(hsv_frame_blk, hsv_frame_blk, mask=mask_blk)\n",
    "        green_filter = cv2.bitwise_and(hsv_frame_grn, hsv_frame_grn, mask=mask_grn)\n",
    "        left_grn_filter = cv2.bitwise_and(hsv_frame_grn_l, hsv_frame_grn_l, mask=mask_grn_l)\n",
    "        right_grn_filter = cv2.bitwise_and(hsv_frame_grn_r, hsv_frame_grn_r, mask=mask_grn_r)\n",
    "        #left and right green images for widget display\n",
    "        left_grn_filter_bgr = cv2.cvtColor(left_grn_filter,cv2.COLOR_HSV2BGR)\n",
    "        right_grn_filter_bgr = cv2.cvtColor(right_grn_filter,cv2.COLOR_HSV2BGR)\n",
    "        left_region_widget.value = bgr8_to_jpeg(left_grn_filter_bgr)\n",
    "        right_region_widget.value = bgr8_to_jpeg(right_grn_filter_bgr)\n",
    "        blk = cv2.cvtColor(black_filter, cv2.COLOR_HSV2BGR)\n",
    "        grn = cv2.cvtColor(green_filter, cv2.COLOR_HSV2BGR)\n",
    "\n",
    "        skel = find_skeletal(blk) #skeletal binary mask for black lines\n",
    "        cgl_x, cgl_y = max_cnt(mask_grn_l) #centroid of left box\n",
    "        cgr_x, cgr_y = max_cnt(mask_grn_r) #centroid of right box\n",
    "        skel_center = linecenter(skel) #centroid of skeletal binary mask\n",
    "        \n",
    "        #Recently done\n",
    "        skel_center_YCoordinate = linecenterYCoordinate(skel)\n",
    "        y_coord_label = widgets.Label(value=f\"Y-Center: {skel_center_YCoordinate}\")\n",
    "#        display(y_coord_label)\n",
    "        \n",
    "        \n",
    "        skel_widget = bgr8_to_jpeg(skel) #for skeletal widget\n",
    "        skel_widget_update.value = skel_widget\n",
    "        grn_widget.value = bgr8_to_jpeg(grn)\n",
    "\n",
    "        \n",
    "        left_sq = 0\n",
    "        right_sq = 0\n",
    "        blk_y = 0\n",
    "        not_straight = 0\n",
    "        left_grn_centroid.value = str(cgl_x) #centroid value for left green\n",
    "        right_grn_centroid.value = str(cgr_x) #centroid value for right green\n",
    "        valid_squares = []\n",
    "        #Check for valid green boxes by taking the centroid's y-axis value, \n",
    "        #cropping the skeletal view's width to just the centroid x-axis and checking if that skeletal segment's centroid is above or below the green centroid\n",
    "        #if so then lock the condition until the boxes are cleared\n",
    "        if cgl_y is not None and not_straight!=1:\n",
    "            skel_copy = skel.copy()\n",
    "            skel_slice = find_skeletal(left_grn_filter_bgr)\n",
    "            blk_y = line_y(skel_slice)\n",
    "            blk_x = find_line_center(skel_slice)\n",
    "            lefty_green.value = str(blk_y)\n",
    "            lefty_black.value = str(cgl_y)\n",
    "            is_empty = np.var(skel_slice) < 1e-6\n",
    "            if blk_y is not None and blk_y != 0:\n",
    "                below_region_widget.value = bgr8_to_jpeg(skel_slice)\n",
    "                if blk_y < cgl_y:\n",
    "                    valid_squares.append(cgl_y)\n",
    "                    #left_region_widget.append()\n",
    "                    left_sq += 1\n",
    "        \n",
    "        if cgr_y is not None and not_straight!=1:\n",
    "            skel_copy = skel.copy()\n",
    "            skel_slice = find_skeletal(right_grn_filter_bgr)\n",
    "            blk_y = line_y(skel_slice)\n",
    "            if blk_y is not None and blk_y != 0:\n",
    "                middle_region_widget.value = bgr8_to_jpeg(skel_slice)\n",
    "                if blk_y < cgr_y:\n",
    "                    valid_squares.append(cgr_y)\n",
    "                    right_sq += 1\n",
    "                    middle_region_black_centroid.value = str(blk_y)\n",
    "        #Set the condition\n",
    "        lefty_green.value = str(len(valid_squares))\n",
    "        if right_sq==1 and left_sq==1:\n",
    "            condition=UTurnLeft\n",
    "            final_message.value = \"U-Turn\"\n",
    "            TurnStatus = 0\n",
    "        elif left_sq==1 and condition is None:\n",
    "            condition = TurnLeftNinetyDegrees\n",
    "            final_message.value = \"Left\"\n",
    "            TurnStatus = 1\n",
    "        elif right_sq==1 and condition is None:\n",
    "            condition = TurnRightNinetyDegrees\n",
    "            final_message.value = \"Right\"\n",
    "            TurnStatus = 2\n",
    "        else:\n",
    "            TurnStatus = 0\n",
    "            #once no more green boxes are detected\n",
    "            if condition is not None and cgl_y is None and cgr_y is None:\n",
    "                condition()\n",
    "                left_sq = 0\n",
    "                right_sq = 0\n",
    "                green_turned = 1\n",
    "                condition = None\n",
    "                final_message.value = \"Straight\"\n",
    "            #if skel_center is None:\n",
    "\n",
    "            if skel_center is None:# and skel_center_YCoordinate is None:\n",
    "                \n",
    "            # Line lost: check how long it's been since we last saw it\n",
    "                if time.time() - last_detected_time > line_lost_timeout:\n",
    "                   robot.stop()\n",
    "                else:\n",
    "                    robot.set_motors(base_speed, base_speed) #Robot go straight\n",
    "                    \n",
    "\n",
    "            else:\n",
    "                    \n",
    "                # Line detected, update time and last center\n",
    "                last_detected_time = time.time()\n",
    "                last_line_center = skel_center\n",
    "                \n",
    "                # Calculate error\n",
    "                error = frame_center_x - skel_center\n",
    "                \n",
    "                # Dynamic speed adjustment for sharp turns\n",
    "#                 if (CloseSharpTurnsFlag == 0):\n",
    "                if abs(error) > sharp_turn_threshold * frame_center_x:\n",
    "                    SharpTurn.value = \"Sharp Turn!\"\n",
    "                    turn = error * turn_sensitivity * 1.75  # Sharpen turn #turn = error * turn_sensitivity * 1.775  # Sharpen turn\n",
    "                    left_speed = base_speed / 2 - turn\n",
    "                    right_speed = base_speed / 2 + turn\n",
    "                else:\n",
    "                    SharpTurn.value = \"Nothing!\"\n",
    "                    turn = error * turn_sensitivity * 1.6\n",
    "                    left_speed = base_speed - turn\n",
    "                    right_speed = base_speed + turn\n",
    "\n",
    "                # Drive the robot\n",
    "                robot.set_motors(left_speed, right_speed)\n",
    "        \n",
    "        \n",
    "        time.sleep(0.01)"
   ]
